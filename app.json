{
  "expo": {
    "name": "Restaurant-App",
    "slug": "Restaurant-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "backgroundColor":"#24180f",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor":"#000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#24180f"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "backgroundColor":"#24180f",
      "bundler": "metro"
    }
  }
}
